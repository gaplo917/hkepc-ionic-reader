{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["standard", "prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly",
    "angular": "readonly",
    "WebViewJavascriptBridge": "writable",
    "ionic": "readonly",
    "moment": "readonly"
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "rules": {
    "standard/no-callback-literal": 0,
    "n/no-callback-literal": 0,
    "no-case-declarations": 0
  }
}
