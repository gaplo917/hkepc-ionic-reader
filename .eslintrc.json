{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": [
    "standard"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly",
    "angular": "readonly",
    "WebViewJavascriptBridge": "writable",
    "ionic": "readonly",
    "moment": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "standard/no-callback-literal": 0
  }
}
